{
  "name": "strider-local",
  "version": "0.0.0",
  "description": "Provides local repositories",
  "main": "worker.js",
  "scripts": {
    "test": "make test"
  },
  "bin": "./bin/setup.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Strider-CD/strider-local"
  },
  "keywords": [
    "strider",
    "plugin",
    "provider",
    "local",
    "git"
  ],
  "author": "Jared Forsyth <jared@jaredforsyth.com>",
