    "webapp": "webapp.js",
    "worker": "worker.js",
    "inline_icon": "code-fork",
    "config": true
  },
  "dependencies": {
    "shell-escape": "0.2.0",
    "mkdirp": "~0.5.1",
    "commander": "2.9.0",
    "crypto": "0.0.3",
    "gravatar": "~1.6.0",
    "strider-git": "git+https://github.com/Strider-CD/strider-git.git",
    "superagent": "~3.4.1",
    "async": "~2.1.4"
  },
  "devDependencies": {
    "jshint": "~2.9.4",
    "mocha": "~3.2.0"
  }
}
