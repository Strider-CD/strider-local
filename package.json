    "worker": "worker.js",
    "inline_icon": "code-fork",
    "config": true
  },
  "dependencies": {
    "shell-escape": "0.0.1",
    "mkdirp": "~0.3.5",
    "commander": "1.3.2",
    "crypto": "0.0.3",
    "gravatar": "~1.0.6",
    "strider-git": "git+https://github.com/Strider-CD/strider-git.git",
    "superagent": "~0.15.7",
    "async": "~0.2.9"
  },
  "devDependencies": {
    "jshint": "~2.3.0",
    "mocha": "~1.13.0"
  }
